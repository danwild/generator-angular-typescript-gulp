  "dependencies": {

	<% if(props.useIcons){ %>
	  "<%= props.iconFont %>": "*",
	<% } %>

	"<%= props.cssFramework %>": "*",
	"angular": "*"

  },
  "overrides": {
    "bootstrap": {
      "main": [
        "less/bootstrap.less",
        "dist/css/bootstrap.css",
